  /{
    jtagtosd {
        compatible = "hisilicon,jtagtosd";
        sdio-ldo = <&ldo9>;
        reg = <0x0 0xFFF0A208 0x0 0x1000>;
        value = <0x01000000>;
        mask = <0x03000000>;
    };

    djtagtosd{
        compatible = "hisilicon,djtagtosd";
        reg = <0x0 0xFFF0A208 0x0 0x1000>;
        value = <0x03000000>;
        mask = <0x03000000>;
    };
  };
