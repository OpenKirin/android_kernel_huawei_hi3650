        compatible = "hisilicon,flp-common";
        reg = <0x0 0xfff19000 0x0 0x1000>;
        interrupts = <0x0 0x3a 0x4>;
        clocks = <&clk_gate_timer5>;
        clock-names = "clk_timer5";
        hisi,flp_switch = "on";    /* on/off */
        status = "ok";
    };
};
