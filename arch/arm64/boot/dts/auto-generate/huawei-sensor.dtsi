	sensorhub_ext_config:sensorhub_ex{
		compatible = "huawei,sensorhub";
		akm_need_current = "no";
		invensense_exist = "no";
		isensor_version = "no";
		adapt_sensor_list_id = <>;
	};

	sensorhub_io_power{
		compatible = "huawei,sensorhub_io";
		//sensor-io-supply = <&lsw51>;
		interrupts = <0 112 4>;
		status="disabled";
	};
};
