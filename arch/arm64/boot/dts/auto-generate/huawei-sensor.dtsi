			als1{
				compatible="huawei,taos-apds990x";
				bus_number=<0>;
				reg=<0x39>;
				chip_id_register=<0x92>;
				chip_id_value=<0x20 0x29 0x30 0x39>;
				file_id=<2>;
				sensor_list_info_id=<3>;
				sensor_type="als";
				//sensor-vdd-supply = <&ldo24>;
				poll_interval=<500>;   //??¡è?31a????£¤?¨¢?D?¡§o?¨¤??????
				threshold_value=<1>;
				GA1=<4166>; //2?¨º¡§¡é?¨¬¡§a?1a?¡§o ?¨º?3??1a???
				GA2=<3900>; //2?¨º¡§¡é?¨¬¡§a?1a?¡§o
				GA3=<4500>; //2?¨º¡§¡é?¨¬¡§a?1a?¡§o
				tp_color =<0>;
				again=<1>;
				gpio_int1=<&gpio25 6 0>;
				atime=<0xdb>;
				init_time = <150>;   //??¡è?31a3?¡§o???£¤¡§o?¨¤??
				status = "disabled";
			};
		};

		ps_sensor:proximity{
			ps1{
				compatible="huawei,taos_apds990x";
				bus_number=<0>;
				reg=<0x39>;
				chip_id_register=<0x92>;
				chip_id_value=<0x20 0x29 0x30 0x39>;
				file_id=<2>;
				sensor_list_info_id=<4>;
				sensor_type="ps";
				//sensor-vdd-supply = <&ldo24>;
				tp_color =<0>;
				min_proximity_value=<750>;
				pwindows_value=<110>;
				pwave_value=<80>;
				threshold_value=<90>;
				ps_pulse_count=<5>;
				persistent=<0x33>;
				rdata_under_sun=<5500>;
				gpio_int1=<&gpio25 6 0>;
				ptime=<0xFF>;
				p_on = <0>;   //??£¤???¡§2??¡§o??¡è?D¡§¡§¡§¡ãa1??¨¤???¨¬??
				poll_interval = <300>;
				init_time = <50>;
				status = "disabled";
			};
		};
		acc_sensor:gsensor{
			acc1{
				compatible="huawei,st-lsm330";
				bus_number=<0>;
				reg=<0x1e>;
				chip_id_register=<0x0f>;
				chip_id_value=<0x40>;
